{
  "name": "font-library",
  "version": "0.0.0",
  "description": "An open source project to tag and organize Google Fonts.",
  "scripts": {
    "test": "eslint . && jsonlint families.json -q && tape test/*.js | tap-min"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.2",
    "eslint": "^6.2.1",
    "husky": "^3.0.4",
    "json-stringify-pretty-compact": "^1.2.0",
    "jsonlint": "^1.6.3",
    "lint-staged": "^9.2.3",
    "prettier": "^1.18.2",
    "request": "^2.88.0",
    "scsslint": "0.0.3",
    "tap-min": "^1.3.0",
    "tape": "^4.11.0"
  },
  "bin": {
    "update-library": "bin/update.js"
  },
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/katydecorah/font-library.git"
  },
  "author": "Katy DeCorah",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/katydecorah/font-library/issues"
  },
  "homepage": "https://github.com/katydecorah/font-library#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.js": [
      "eslint",
      "prettier --single-quote --write",
      "git add"
    ],
    "**/*.md": [
      "prettier --write",
      "git add"
    ],
    "**/*.yml": [
      "prettier --write",
      "git add"
    ],
    "**/*.scss": [
      "scss-lint",
      "prettier --write",
      "git add"
    ],
    "**/*.json": [
      "prettier --print-width 500  --write",
      "git add"
    ]
  },
  "dependencies": {}
}
